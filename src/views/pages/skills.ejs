<%- include('../template/header')-%>

<%- include('../template/navbar')-%>


<div class="container p-4">
  <div class="row">
      <div class="col-md-6 col-sm-12 mb-4">
        <div class="card">
            <div class="card-header">
              <h3> Nuevos Skills </h3>
            </div>
            <div class="card-body">
                <form action="/skills" method="POST" >
                    <div class="form-group">
                        <input type="text" name="urlImage" class="form-control" placeholder="Url Logo Tecnologia">
                    </div>
                    <div class="form-group">
                        <input type="text" name="descriptionImagen" class="form-control" placeholder="Nombre Tecnologia">
                    </div>
                    <button type="submit" class="btn btn-primary btn-block">
                        Registrar
                    </button>
                </form>
            </div>
        </div>
      </div>
      <div class="col-md-6 col-sm-12">
        <table class="table ">
            <thead class="thead-dark">
              <tr>
                <th>#</th>
                <th>First Name</th>
                <th>Last Name</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <th scope="row">1</th>
                <td>Mark</td>
                <td>Otto</td>
              </tr>
            </tbody>
          </table>
          
      </div>
  </div>
  
</div>


<%- include('../template/footer')-%>